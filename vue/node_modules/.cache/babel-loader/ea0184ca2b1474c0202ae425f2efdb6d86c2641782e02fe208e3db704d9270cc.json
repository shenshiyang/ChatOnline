{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      params: {\n        name: '',\n        phone: '',\n        pageNum: 1,\n        pageSize: 5\n      },\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      tableData: [],\n      total: 0,\n      dialogFormVisible: false,\n      form: {}\n    };\n  },\n  // 页面加载的时候，做一些事情，在created里面\n  created() {\n    this.findBySearch();\n  },\n  // 定义一些页面上控件出发的事件调用的方法\n  methods: {\n    /**\r\n     * 添加\r\n     */\n    handleTianjia(row) {\n      this.$prompt('请输入内容', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消'\n      }).then(({\n        value\n      }) => {\n        let data = {\n          nr: value,\n          hyUserId: row.id\n        };\n        request.post(\"/hy/add\", data).then(res => {\n          if (res.code === '0') {\n            this.$message.success(\"提交成功!\");\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n      }).catch(() => {});\n    },\n    findBySearch() {\n      request.get(\"/user/search\", {\n        params: this.params\n      }).then(res => {\n        if (res.code === '0') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        } else {\n          this.$message({\n            message: res.msg,\n            type: 'success'\n          });\n        }\n      });\n    },\n    add() {\n      this.form = {};\n      this.dialogFormVisible = true;\n    },\n    edit(obj) {\n      this.form = obj;\n      this.dialogFormVisible = true;\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 5,\n        name: '',\n        phone: ''\n      };\n      this.findBySearch();\n    },\n    handleSizeChange(pageSize) {\n      this.params.pageSize = pageSize;\n      this.findBySearch();\n    },\n    handleCurrentChange(pageNum) {\n      this.params.pageNum = pageNum;\n      this.findBySearch();\n    },\n    submit() {\n      request.post(\"/user\", this.form).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            message: '操作成功',\n            type: 'success'\n          });\n          this.dialogFormVisible = false;\n          this.findBySearch();\n        } else {\n          this.$message({\n            message: res.msg,\n            type: 'error'\n          });\n        }\n      });\n    },\n    del(id) {\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            message: '删除成功',\n            type: 'success'\n          });\n          this.findBySearch();\n        } else {\n          this.$message({\n            message: res.msg,\n            type: 'success'\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","data","params","name","phone","pageNum","pageSize","user","localStorage","getItem","JSON","parse","tableData","total","dialogFormVisible","form","created","findBySearch","methods","handleTianjia","row","$prompt","confirmButtonText","cancelButtonText","then","value","nr","hyUserId","id","post","res","code","$message","success","error","msg","catch","get","list","message","type","add","edit","obj","reset","handleSizeChange","handleCurrentChange","submit","del","delete"],"sources":["src/views/User.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-bottom: 15px\">\r\n      <el-input v-model=\"params.name\" style=\"width: 200px\" placeholder=\"请输入姓名\"></el-input>\r\n      <el-input v-model=\"params.phone\" style=\"width: 200px; margin-left: 5px\" placeholder=\"请输入电话\"></el-input>\r\n      <el-button type=\"warning\" style=\"margin-left: 10px\" @click=\"findBySearch()\">查询</el-button>\r\n      <el-button type=\"warning\" style=\"margin-left: 10px\" @click=\"reset()\">清空</el-button>\r\n      <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"add()\">新增</el-button>\r\n    </div>\r\n    <div>\r\n      <el-table border :data=\"tableData\" style=\"width: 100%\">\r\n        <el-table-column align=\"center\" label=\"头像\">\r\n          <template v-slot=\"scope\">\r\n            <el-image :src=\"scope.row.avatar\" :preview-src-list=\"[scope.row.avatar]\"\r\n              style=\"width: 50px; height: 50px; border-radius: 50%\"></el-image>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" prop=\"name\" label=\"姓名\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"sex\" label=\"性别\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"age\" label=\"年龄\"></el-table-column>\r\n        <el-table-column align=\"center\" prop=\"phone\" label=\"电话\"></el-table-column>\r\n<!--        <el-table-column align=\"center\" prop=\"role\" label=\"分组\"></el-table-column>-->\r\n        <el-table-column align=\"center\" label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"success\" plain @click=\"handleTianjia(scope.row)\">添加</el-button>\r\n            <template v-if=\"user.role === 'ADMIN'\">\r\n              <el-button type=\"primary\" @click=\"edit(scope.row)\">编辑</el-button>\r\n              <el-popconfirm title=\"确定删除吗？\" @confirm=\"del(scope.row.id)\">\r\n                <el-button slot=\"reference\" type=\"danger\" style=\"margin-left: 5px\">删除</el-button>\r\n              </el-popconfirm>\r\n            </template>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <div style=\"margin-top: 10px\">\r\n      <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n        :current-page=\"params.pageNum\" :page-sizes=\"[5, 10, 15, 20]\" :page-size=\"params.pageSize\"\r\n        layout=\"total, sizes, prev, pager, next\" :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <div>\r\n      <el-dialog title=\"请填写信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\r\n        <el-form :model=\"form\">\r\n          <el-form-item label=\"姓名\" label-width=\"15%\">\r\n            <el-input v-model=\"form.name\" autocomplete=\"off\" style=\"width: 90%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"性别\" label-width=\"15%\">\r\n            <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\r\n            <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\r\n          </el-form-item>\r\n          <el-form-item label=\"年龄\" label-width=\"15%\">\r\n            <el-input v-model=\"form.age\" autocomplete=\"off\" style=\"width: 90%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"电话\" label-width=\"15%\">\r\n            <el-input v-model=\"form.phone\" autocomplete=\"off\" style=\"width: 90%\"></el-input>\r\n          </el-form-item>\r\n<!--          <el-form-item label=\"分组\" label-width=\"15%\">-->\r\n<!--            <el-select v-model=\"form.role\" placeholder=\"请选择\" style=\"width: 90%\">-->\r\n<!--              <el-option label=\"领导\" value=\"TEACHER\"></el-option>-->\r\n<!--              <el-option label=\"员工\" value=\"STUDENT\"></el-option>-->\r\n<!--            </el-select>-->\r\n<!--          </el-form-item>-->\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"submit()\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      params: {\r\n        name: '',\r\n        phone: '',\r\n        pageNum: 1,\r\n        pageSize: 5\r\n      },\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      dialogFormVisible: false,\r\n      form: {}\r\n    }\r\n  },\r\n  // 页面加载的时候，做一些事情，在created里面\r\n  created() {\r\n    this.findBySearch();\r\n  },\r\n  // 定义一些页面上控件出发的事件调用的方法\r\n  methods: {\r\n    /**\r\n     * 添加\r\n     */\r\n    handleTianjia(row) {\r\n      this.$prompt('请输入内容', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n      }).then(({ value }) => {\r\n        let data = {\r\n          nr: value,\r\n          hyUserId: row.id,\r\n        }\r\n        request.post(\"/hy/add\", data).then(res => {\r\n          if (res.code === '0') {\r\n            this.$message.success(\"提交成功!\");\r\n          } else {\r\n            this.$message.error(res.msg);\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    findBySearch() {\r\n      request.get(\"/user/search\", {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '0') {\r\n          this.tableData = res.data.list;\r\n          this.total = res.data.total;\r\n        } else {\r\n          this.$message({\r\n            message: res.msg,\r\n            type: 'success'\r\n          });\r\n        }\r\n      })\r\n    },\r\n    add() {\r\n      this.form = {};\r\n      this.dialogFormVisible = true;\r\n    },\r\n    edit(obj) {\r\n      this.form = obj;\r\n      this.dialogFormVisible = true;\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 5,\r\n        name: '',\r\n        phone: ''\r\n      }\r\n      this.findBySearch();\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.params.pageSize = pageSize;\r\n      this.findBySearch();\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.params.pageNum = pageNum;\r\n      this.findBySearch();\r\n    },\r\n    submit() {\r\n      request.post(\"/user\", this.form).then(res => {\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            message: '操作成功',\r\n            type: 'success'\r\n          });\r\n          this.dialogFormVisible = false;\r\n          this.findBySearch();\r\n        } else {\r\n          this.$message({\r\n            message: res.msg,\r\n            type: 'error'\r\n          });\r\n        }\r\n      })\r\n    },\r\n    del(id) {\r\n      request.delete(\"/user/\" + id).then(res => {\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            message: '删除成功',\r\n            type: 'success'\r\n          });\r\n          this.findBySearch();\r\n        } else {\r\n          this.$message({\r\n            message: res.msg,\r\n            type: 'success'\r\n          });\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA0EA,OAAAA,OAAA;AAEA;EACAC,KAAA;IACA;MACAC,MAAA;QACAC,IAAA;QACAC,KAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,SAAA;MACAC,KAAA;MACAC,iBAAA;MACAC,IAAA;IACA;EACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;EACA;EACA;EACAC,OAAA;IACA;AACA;AACA;IACAC,cAAAC,GAAA;MACA,KAAAC,OAAA;QACAC,iBAAA;QACAC,gBAAA;MACA,GAAAC,IAAA;QAAAC;MAAA;QACA,IAAAxB,IAAA;UACAyB,EAAA,EAAAD,KAAA;UACAE,QAAA,EAAAP,GAAA,CAAAQ;QACA;QACA5B,OAAA,CAAA6B,IAAA,YAAA5B,IAAA,EAAAuB,IAAA,CAAAM,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;UACA;YACA,KAAAD,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;UACA;QACA;MACA,GAAAC,KAAA,QACA;IACA;IACAnB,aAAA;MACAjB,OAAA,CAAAqC,GAAA;QACAnC,MAAA,OAAAA;MACA,GAAAsB,IAAA,CAAAM,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAnB,SAAA,GAAAkB,GAAA,CAAA7B,IAAA,CAAAqC,IAAA;UACA,KAAAzB,KAAA,GAAAiB,GAAA,CAAA7B,IAAA,CAAAY,KAAA;QACA;UACA,KAAAmB,QAAA;YACAO,OAAA,EAAAT,GAAA,CAAAK,GAAA;YACAK,IAAA;UACA;QACA;MACA;IACA;IACAC,IAAA;MACA,KAAA1B,IAAA;MACA,KAAAD,iBAAA;IACA;IACA4B,KAAAC,GAAA;MACA,KAAA5B,IAAA,GAAA4B,GAAA;MACA,KAAA7B,iBAAA;IACA;IACA8B,MAAA;MACA,KAAA1C,MAAA;QACAG,OAAA;QACAC,QAAA;QACAH,IAAA;QACAC,KAAA;MACA;MACA,KAAAa,YAAA;IACA;IACA4B,iBAAAvC,QAAA;MACA,KAAAJ,MAAA,CAAAI,QAAA,GAAAA,QAAA;MACA,KAAAW,YAAA;IACA;IACA6B,oBAAAzC,OAAA;MACA,KAAAH,MAAA,CAAAG,OAAA,GAAAA,OAAA;MACA,KAAAY,YAAA;IACA;IA